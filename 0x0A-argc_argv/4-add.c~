#include <stdlib.h>
#include <stdio.h>

/**
 * main - Add positive numbers
 *@argc: arguments
 *@argv: array pointing to arguments
 *
 *Return: Always 0.
 */
int main(int argc, char **argv)
{
	int i, sum = 0;

	if (arg < 1)
		return (0);

	for (i = 1; i < argc; i++)
	{
		if (!atoi(argv[i]))
		{
			printf("%s\n", "Error");
			return (1);
		}
		sum += atoi(argv[i]);
	}
	printf("%d\n", sum);

	return (0);
}
